      <body>
         <div>
            <p>INCIPIUNT CAPITULA CHRONICI HIERONIMI EXCARPSUM</p>
            <table>
               <row n="1">
                  <cell><num>I</num></cell>
                  <cell>De regnum <rs type="ethnic"><choice><abbr>assrriorum</abbr><expan>assyrriorum</expan></choice></rs></cell>
               </row>
               <row n="2">
                  <cell><num>II</num></cell>
                  <cell>De natiuitate <persName><choice><abbr>abraha</abbr><expan>abrahae</expan></choice></persName> et <choice><abbr>genr</abbr><expan>generis</expan></choice> <choice><abbr>ne</abbr><expan>unknown</expan></choice> eius <choice><abbr>usq</abbr><expan>usque</expan></choice>. ad <choice><abbr>morsen</abbr><expan>unknown</expan></choice></cell>
               </row>
               <row n="3">
                  <cell><num>III</num></cell>
                  <cell>De <choice><abbr>morsen</abbr><expan>morsen</expan></choice> et <rs type="ethnic">iudices</rs> super <choice><abbr>irt</abbr><expan>unknown</expan></choice>.</cell>
               </row>
               <row n="4">
                  <cell><num>IIII</num></cell>
                  <cell>De captiuitate <placeName><w>tro<unclear>i</unclear>ae</w></placeName> et <choice><abbr>iniciu</abbr><expan>inicium</expan></choice> <rs type="ethnic">francorum</rs> et <rs type="ethnic"><choice><abbr>romanr</abbr><expan>romanorum</expan></choice></rs></cell>
               </row>
               <row n="5">
                  <cell><num>V</num></cell>
                  <cell>De <rs type="ethnic">francione</rs> rege <rs type="ethnic"><choice><abbr>francoru</abbr><expan>francorum</expan></choice></rs> et <rs type="ethnic">francis</rs>.</cell>
               </row>
               <row n="6">
                  <cell><num>VI</num></cell>
                  <cell>De inicium ducum <rs type="ethnic">francorum</rs>.</cell>
               </row>
               <row n="7">
                  <cell><num>VII</num></cell>
                  <cell>De <rs type="ethnic">hebreis</rs> et eorum <rs type="ethnic">iudices</rs></cell>
               </row>
               <row n="8">
                  <cell><num>VIII</num></cell>
                  <cell>De captiuitatem <placeName>troiae</placeName> et <choice><abbr>egressione</abbr><expan>egressionem</expan></choice> exinde <persName>priamo</persName> et <persName>friga</persName> unde <rs type="ethnic">romani</rs> et <rs type="ethnic">franci</rs> fuerunt.</cell>
               </row>
		<row n="9">
                  <cell>VIIII</cell>
                  <cell>De inicium regis <rs type="ethnic">romanorum</rs>.</cell>
               </row>
               <row n="10">
                  <cell>X</cell>
                  <cell>De aedificationem <rs type="ethnic">chartagenis</rs> et supputationem annorum ad <persName>adam</persName> usque ad <persName>moysen</persName>.</cell>
               </row>
               <row n="11">
                  <cell>XI</cell>
                  <cell>De regibus <rs type="ethnic">hebreorum</rs> irt</cell>
               </row>
               <row n="12">
                  <cell>XII</cell>
                  <cell>De regem <rs type="ethnic">latinorum</rs></cell>
               </row>
               <row n="13">
                  <cell>XIII</cell>
                  <cell>De reges <rs type="ethnic">hebreorum</rs>.</cell>
               </row>
               <row n="14">
                  <cell>XIIII</cell>
                  <cell>De <choice><abbr>iniciu</abbr><expan>inicium</expan></choice> nominis <placeName>rome</placeName> <choice><abbr>antequa</abbr><expan>antequam</expan></choice> condita fuisset.</cell>
               </row>
               <row n="15">
                  <cell>XV</cell>
                  <cell>De <rs type="ethnic">assyriorum</rs> imperio distructum <choice><abbr>qd</abbr><expan>quod</expan></choice> stetit  <choice><abbr>an</abbr><expan>annis</expan></choice> mit <num>ccxlii</num>.</cell>
               </row>
               <row n="16">
                  <cell>XVI</cell>
                  <cell>De <rs type="ethnic">latinorum</rs> et <placeName>roma</placeName> a <persName>romolo</persName> condita.</cell>
               </row>
               <row n="17">
                  <cell>XVII</cell>
                  <cell>De <persName>nabuchodonosor</persName> rege et <choice><abbr>captiuitate</abbr><expan>captiuitatem</expan></choice> <rs type="ethnic">iudaeorum</rs>.</cell>
               </row>
               <row n="18">
                  <cell>XVIII</cell>
                  <cell>De <choice><abbr>captiuitate</abbr><expan>captiuitatem</expan></choice> <placeName>trogae</placeName> et <sic>olrmpiadem</sic><corr>olympiadem</corr> primam.</cell>
               </row>
               <row n="19">
                  <cell>XVIIII</cell>
                  <cell>De <rs type="ethnic">macedonum</rs> rege <persName><sic>phrlippo</sic><corr>phylippo</corr></persName>  <choice><abbr>msibus</abbr><expan>unknown</expan></choice> <choice><abbr>ann</abbr><expan>annis</expan></choice> conditus.</cell>
               </row>
               <row n="20">
                  <cell>XX</cell>
                  <cell>De <persName>tarquinio prisco</persName> et numerum sentorum <sic>romcce</sic>.</cell>
               </row>
                <row n="21">
                  <cell><num>XXI</num></cell>
                  <cell>De captiuitatem <rs type="ethnic">iudaeorum</rs> ab <persName>abuchodonosor</persName> rege</cell>
               </row>
               <row n="22">
                  <cell><num>XXII</num></cell>
                  <cell>De <w>ampliata<del>m</del></w> <placeName><w>roma<del>m</del></w></placeName></cell>
               </row>
               <row n="23">
                  <cell><num>XXIII</num></cell>
                  <cell>De morte <persName>nabuchodonosor</persName> rege</cell>
               </row>
               <row n="24">
                  <cell><num>XXIIII</num></cell>
                  <cell>De regressione <rs type="ethnic">iudaeorum</rs>  <rs type="ethnic">perorrum</rs> <rs>persarum</rs> </cell>
               </row>
               <row n="25">
                  <cell><num>XXV</num></cell>
                  <cell>De regnum <persName>tarquini</persName> <placeName>romam</placeName>.</cell>
               </row>
               <row n="26">
                  <cell><num>XXVI</num></cell>
                  <cell>De <persName>nabuchodonosor</persName> subcuius <choice><abbr>tempr</abbr><expan>temporem</expan></choice> <persName><w>oloferni<del>i</del>s</w></persName> fuit.</cell>
               </row>
               <row n="27">
                  <cell><num>XXVII</num></cell>
                  <cell>De inicium <choice><abbr>consulu</abbr><expan>consulum</expan></choice> <placeName>romam</placeName> qui <choice><abbr>reger</abbr><expan>unknown</expan></choice> <choice><abbr>imperiu</abbr><expan>imperium</expan></choice> <choice><abbr>an</abbr><expan>annis</expan></choice> <num>CCCCLX</num></cell>
               </row>
               <row n="28">
                  <cell><num>XVIII</num></cell>
                  <cell>De <persName>alexandro magno</persName> <rs type="ethnic">macedo</rs>.</cell>
               </row>
               <row n="29">
                  <cell><num>XVIIII</num></cell>
                  <cell>De morte <persName>alexandri</persName> et <choice><abbr>translatu</abbr><expan>translatum</expan></choice> <choice><abbr>regnu</abbr><expan>regnum</expan></choice> eius in pluris <choice><abbr>regib</abbr><expan>regibus</expan></choice>.</cell>
               </row>
               <row n="30">
                  <cell><num>XXX</num></cell>
                  <cell>De <rs type="ethnic">romanis</rs> qui plures gentes superant consules habentes.</cell>
               </row>
               <row n="31">
                  <cell><num>XXXI</num></cell>
                  <cell>DE IMICIUM IMPERATORUM</cell>
               </row>
               <row n="32">
                  <cell><num>XXXII</num></cell>
                  <cell>De <persName>gaium iulium</persName> qui monarciam tenuit</cell>
               </row>
               <row n="33">
                  <cell><num>XXXIII</num></cell>
                  <cell>De <persName>octaui</persName> <choice><abbr>anu</abbr><expan>annum</expan></choice> qui <choice><abbr>monarcia</abbr><expan>monarciam</expan></choice> tenuit. In cuius <choice><abbr>tempr</abbr><expan>tempore</expan></choice> <choice><abbr>dns</abbr><expan>dominus</expan></choice> <choice><abbr>nt</abbr><expan>unknown</expan></choice> <persName><choice><abbr>i</abbr><expan>ihesus</expan></choice> <choice><abbr>x</abbr><expan>christus</expan></choice></persName>natus <choice><abbr>e</abbr><expan>est</expan></choice>. </cell>
               </row>
               <row n="34">
                  <cell><num>XXXIIII</num></cell>
                  <cell>De <choice><abbr>supputatione</abbr><expan>supputationem</expan></choice> <choice><abbr>annr</abbr><expan>annorum</expan></choice>  ab <persName>adam</persName> <choice><abbr>usq</abbr><expan>usque</expan></choice>. ad <choice><abbr>pdicatione</abbr><expan>praedicationem</expan></choice> <persName><choice><abbr>xpi</abbr><expan>christi</expan></choice></persName></cell>
               </row>
               <row n="35">
                  <cell><num>XXXV</num></cell>
                  <cell>De imperium <persName>gagi</persName> et <persName>claudiae</persName>.</cell>
               </row>
            </table>
         </div>
